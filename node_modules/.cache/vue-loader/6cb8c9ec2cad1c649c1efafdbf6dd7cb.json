\t\tref=\"fileInput\" \n\t\t\t\t\taccept=\"image/*\" @change=\"onFilePicked\">\n            </div>\n          </div>\n        </v-col>\n        <v-col cols=7 class=\"pl-0\">\n          <div class=\"name-section\">\n              <v-text-field\n                v-model=\"name\"\n                single-line\n                disabled\n                :rules=\"nameRules\"\n              >Rohini</v-text-field>\n                <v-text-field\n                v-model=\"lastName\"\n                 :rules=\"lastNameRules\"\n                single-line\n                disabled\n              >Verma</v-text-field>\n          </div>\n        </v-col>\n      </v-row>\n      <v-row class=\"mx-auto account-another-info\">\n        <v-col cols=12>\n          <v-text-field\n              label=\"User Name\"\n              disabled\n              v-model=\"userName\"\n              :rules=\"userNameRules\"\n            ></v-text-field>\n        </v-col>\n        <v-col cols=12>\n          <v-text-field\n              label=\"Email\"\n              disabled\n              v-model=\"email\"\n              :rules=\"emailRules\"\n            ></v-text-field>\n        </v-col>\n        <v-col cols=12>\n          <v-text-field\n              label=\"Password\"\n              disabled\n              v-model=\"password\"\n            ></v-text-field>\n        </v-col>\n        <v-col cols=12>\n          <v-text-field\n              label=\"Phone Number\"\n              disabled\n              v-model=\"phoneNumber\"\n            ></v-text-field>\n        </v-col>\n         <v-col cols=12>\n          <!-- <v-text-field\n              label=\"Address\"\n              disabled\n              v-model=\"address\"\n            ></v-text-field> -->\n            <v-textarea\n                  label=\"Address\"\n              disabled\n              v-model=\"address\"\n          rows=\"2\"\n          row-height=\"20\"\n        ></v-textarea>\n        </v-col>\n      </v-row>\n      </v-card>\n    </v-container>\n </div>\n</template>\n\n<script>\nexport default {\n  name: 'account-form',\n  data(){\n    return{\n\t  image:null,\t\n      name:\"John\",\n\t  imageUrl:\"https://cdn.vuetifyjs.com/images/john.png\",\n      nameRules: [\n        v => !!v || 'Name is required',\n        v => (v && v.length <= 10) || 'Name must be less than 10 characters',\n      ],\n      lastName:\"Verma\",\n      lastNameRules: [\n        v => !!v || 'Last Name is required',\n        v => (v && v.length <= 15) || 'Last Name must be less than 15 characters',\n      ],\n      userName:\"JhonMaster045\",\n      userNameRules: [\n        v => !!v || 'User Name is required',\n        v => (v && v.length <= 15) || 'User Name must be less than 15 characters',\n      ],\n      email: 'JohnVerma045@wohlig.com',\n      emailRules: [\n        v => !!v || 'E-mail is required',\n        v => /.+@.+\\..+/.test(v) || 'E-mail must be valid',\n      ],\n      password:\"**********\",\n      phoneNumber:\"9619793665\",\n      address:\"Amba Bhavan, Level 3 B Wing, Plot No 30, Sion Circle(E)\"\n    }\n  },\n  methods: {\n\tpicFile(){\n\t\tthis.$refs.fileInput.click()\n\t},\n\tonFilePicked(event) {\n\t\tconst files = event.target.files;\n\t\tlet filename = files[0].name;\n\t\tif(filename.lastIndexOf('.') <= 0) {\n\t\t\treturn alert('Please add a valid file');\n\t\t}\n\t\tconst fileReader = new FileReader()\n\t\tfileReader.addEventListener('load', () => {\n\t\t\tthis.imageUrl = fileReader.result\n\t\t})\n\t\tfileReader.readAsDataURL(files[0])\n\t\tthis.image = files[0];\n\t}\n  }\n};\n</script>\n<style lang=\"scss\">\n@import '@/assets/scss/Home.scss';\n\n.account-details-section{\n  margin-top:20px;\n  .v-card:not(.v-sheet--tile):not(.v-card--shaped) {\n    border-radius: 17px;\n  }\n  .logger-img-section{\n    margin:auto;\n      .logger-img{\n      .v-image{\n        width:75px;\n        height:75px;\n        border-radius:50%;\n      }\n      .edit-photo-section{\n        margin-top:3px;\n        .v-btn{\n          width: 100%;\n          border-radius: 20px;\n        }\n        .v-btn:not(.v-btn--flat):not(.v-btn--text):not(.v-btn--outlined) {\n          background-color: $white !important;\n        }\n      }\n    }\n  }\n  .name-section{\n    margin-top: 20px;\n  }\n  .account-another-info{\n    padding:20px 0;\n  }\n    .v-input .v-input__control .v-input__slot .v-text-field__slot{\n      border-bottom: 1px solid $date-grey;\n      input{\n        border: 0px solid $grey;\n        font-size: 18px;\n      }\n      .v-label{\n        font-size:22px;\n        top:-3px;\n        text-transform: uppercase;\n        color:$grey;\n      }\n    }\n    [disabled] {\n    cursor: default;\n    color: $black !important;\n    }\n    .v-text-field {\n      padding-top: 0px;\n      margin-top: 0px;\n    }\n}\n</style>\n"]}]}