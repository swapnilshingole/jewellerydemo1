t\t\t\t</v-col>\n\t\t\t</v-row>\n\t\t</v-form>\n   </div>\n    </v-container>\n    </div>\n</template>\n<script>\nexport default {\n  name: 'forgot-password-section',\n  data() {\n    return {\n\t\temailRules: [\n          (v) => !!v || 'E-mail is required',\n          (v) => /^\\w+([\\.-]?\\w+)*@\\w+([\\.-]?\\w+)*(\\.\\w{2,3})+$/.test(v) || 'E-mail must be valid'\n        ]\n    };\n  },\n  methods: {\n      submit () {\n\t\tvar flag = this.$refs.form.validate()\n\t\t\n\t\tif(flag){\n\t\t\tconsole.log(\"redirect\");\n\t\t\tthis.$router.push('/product')\n\t\t}\n      }\n    },\n};\n</script>\n<style lang=\"scss\">\n@import '@/assets/scss/Home.scss';\n.forgot-password-section{\n  .forgot-password-content{\n    margin: 154px 37px 0 37px;\n    .head-text{\n      font-size: 20px;\n      font-weight: 400;\n      color:$white;\n    }\n    .blockquote{\n       color:$white;\n       font-size: 14px;\n       font-weight: 400;\n    }\n    .v-input input {\n      border:0px solid $white;\n    }\n    .theme--light.v-label {\n      color: $white;\n    }\n\n      .primary--text {\n        color:$white !important;\n        caret-color: $white !important;\n      }\n      .theme--light.v-input input, .theme--light.v-input textarea {\n          color:$white !important;\n      }\n      .theme--light.v-text-field > .v-input__control > .v-input__slot:before {\n           border-color: $white;\n        }\n\n        .btn-resetpass{\n        margin-top:40px;\n        margin-bottom: 100px;\n         :focus {\n           background: $white !important;\n            }\n        .v-btn{\n            font-size:20px;\n            font-weight: 500;\n            width: 100%;\n            height: 50px;\n            text-transform: capitalize;\n            color: $white;\n            background-color:$active-btn-yellow!important;\n            :focus {\n           background: $white !important;\n            }\n            :hover {\n             background: $active-btn-yellow!important;\n            }\n        }\n         .v-btn:not(.v-btn--text):not(.v-btn--outlined):hover:before {\n            opacity: 0;\n        }\n    }\n  }\n}\n.back {\n  background: url(../assets/apptheme.jpg) rgba(0, 0, 0, 0.5) no-repeat center center;\n  background-size: cover;\n  height: 100vh;\n}\n</style>\n"]}]}