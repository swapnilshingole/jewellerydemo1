AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"Product-Timelines.vue","sourceRoot":"src/components/Product","sourcesContent":["<template>\n  <div class=\"product-timeline\">\n      <v-timeline align-top :dense=\"$vuetify.breakpoint.smAndDown\">\n    <v-timeline-item\n      v-for=\"(item, i) in items\"\n      :key=\"i\"\n      :color=\"item.color\"\n      :icon=\"item.icon\"\n      fill-dot\n       fill-dot2\n    >\n    <div class=\"date-section\">\n        <div class=\"small-dot\">\n        </div>\n        <div class=\"date-point\">\n           {{item.date}}\n        </div>\n    </div>\n      <v-card\n        :color=\"item.color\"\n        class=\"v-card-color\"\n      >\n        <v-card-text class=\"white text--primary cart-text\">\n          <div>\n            {{item.idNumber}}\n          </div>\n          <div>\n            {{item.Name}}\n          </div>\n          <div v-if=\"item.Type\" >\n            Type: {{item.Type}}\n          </div>\n          <div v-if=\"item.BoughtFrom\">\n              Bought from:{{item.BoughtFrom}}\n          </div>\n          <div v-if=\"item.RestorationWork\">\n             Restoration Work:{{item.RestorationWork}}\n          </div>\n          <div v-if=\"item.UsedTill\">\n              Used Till:{{item.UsedTill}}\n          </div>\n          <div v-if=\"item.SoldTo\">\n              Sold To:{{item.SoldTo}}\n          </div>\n        </v-card-text>\n          <div class=\"prod-img-section\">\n            <v-img\n                class=\"product-img product-img-1 polaroid rotate_left\"\n                :src=\"item.img1\"\n                height=\"55px\"\n                width=\"55px\"\n\n            ></v-img>\n                <v-img\n                class=\"product-img product-img-2 polaroid rotate_right\"\n                :src=\"item.img2\"\n                height=\"55px\"\n                width=\"55px\"\n            ></v-img>\n          </div>\n\n      </v-card>\n    </v-timeline-item>\n  </v-timeline>\n  </div>\n</template>\n<script>\nimport img1 from '../../assets/img/r-1.jpg';\nimport img2 from '../../assets/img/r-2.jpg';\nimport img3 from '../../assets/img/r-3.jpg';\nimport img4 from '../../assets/img/r-4.jpg';\n\nexport default {\n  data: () => ({\n    items: [\n      {\n        color: '#bdbdbd',\n        icon: 'mdi-star',\n        date: '20 March 2020',\n        idNumber: 'XA1234567',\n        Name: 'Virendra Shahaney',\n        Type: 'Silver Stering',\n        BoughtFrom: 'India',\n        RestorationWork: '',\n        UsedTill: '',\n        SoldTo: '',\n        img1,\n        img2,\n      },\n      {\n        color: '#bdbdbd',\n        icon: 'mdi-book-variant',\n        date: '10 March 2020',\n        idNumber: 'XA1344455',\n        Name: 'Rajiv Gupta',\n        Type: 'Silver Stering',\n        BoughtFrom: 'UK',\n        RestorationWork: 'Chintamani',\n        UsedTill: 'Virendra Shahaney',\n        SoldTo: 'Rajiv Gupta',\n        img1: img3,\n        img2: img4,\n      },\n      {\n        color: '#bdbdbd',\n        icon: 'mdi-airballoon',\n        date: '20 Feb 2020',\n        idNumber: 'XA123u94i',\n        Name: 'Virendra Shahaney',\n        Type: 'Silver Stering',\n        BoughtFrom: 'London',\n        RestorationWork: 'Chintamani',\n        UsedTill: 'Rajiv Gupta',\n        SoldTo: 'Virendra Shahanev',\n        img1: img3,\n        img2,\n      },\n      {\n        color: '#bdbdbd',\n        icon: 'mdi-buffer',\n        date: '20 Jan 2020',\n        idNumber: 'XA12099984',\n        Name: 'Virendra Shahaney',\n        Type: 'Silver Stering',\n        BoughtFrom: 'London',\n        RestorationWork: 'Chintamani',\n        UsedTill: 'Rajiv Gupta',\n        SoldTo: 'Virendra Shahanev',\n        img1,\n        img2: img4,\n      },\n    ],\n  }),\n};\n</script>\n<style lang=\"scss\">\n@import '@/assets/scss/Home.scss';\n\n.product-timeline{\n    .v-timeline:before {\n        bottom: 0;\n        content: \"\";\n        height: 100%;\n        position: absolute;\n        top: 27px;\n        width: 2px;\n    }\n    .date-section{\n        position: relative;\n        .small-dot{\n            position: absolute;\n                height: 20px;\n                left: -47px;\n                width: 20px;\n                background-color: $active-btn-yellow !important;\n                border-color: $active-btn-yellow !important;\n                border-radius: 50%;\n                display: flex;\n                justify-content: center;\n                align-items: center;\n        }\n        .date-point{\n            margin-left: -14px;\n            font-size: 16px;\n        }\n    }\n    .cart-text{\n        font-size:11px;\n         border-radius:20px;\n    }\n    .v-card:not(.v-sheet--tile):not(.v-card--shaped) {\n        border-radius: 23px;\n    }\n   .v-timeline--dense:not(.v-timeline--reverse):before {\n        left: calc(24px - 1px);\n        right: initial;\n    }\n    .v-timeline--dense .v-timeline-item__body {\n        max-width: calc(100% - 63px);\n    }\n    .v-timeline-item__divider {\n        display: block;\n        //         width: 20px;\n        // max-width: 20px;\n        min-width:0px;\n\n        .v-timeline-item__dot{\n           left: -62px;\n            top: 43px;\n            position: absolute;\n            height: 51px;\n            width: 51px;\n        }\n    }\n    .v-timeline-item__body > .v-card:not(.v-card--flat):before, .v-timeline-item__body >\n     .v-card:not(.v-card--flat):after {\n        border-top: 0px solid transparent;\n        border-bottom: 0px solid transparent;\n        border-right: 0px solid black;\n        z-index: 0;\n        opacity:0;\n    }\n    .v-card-color{\n      background-color:$white !important;\n      border-color: $white !important;\n    }\n    .v-timeline--dense:not(.v-timeline--reverse):before,\n .v-application--is-rtl\n  .v-timeline--reverse.v-timeline--dense:before {\n    right: auto;\n    left: 25px !important;\n}\n\n}\n.v-card-color{\n  position: relative;\n.prod-img-section{\n  position: absolute;\n  left:150px;\n  top:6px;\n      // margin: 5px -24px;\n  .product-img-2{\n            margin: -5px 36px;\n  }\n  .polaroid {\n  width: 284px;\n  padding: 10px 10px 20px 10px;\n  border: 1px solid #BFBFBF;\n  background-color: $white;\n  box-shadow: 5px 5px 5px #aaaaaa;\n}\n.rotate_right {\n  float: left;\n  -ms-transform: rotate(7deg); /* IE 9 */\n  -webkit-transform: rotate(7deg); /* Safari */\n  transform: rotate(7deg);\n}\n.rotate_left {\n  float: left;\n  -ms-transform: rotate(-8deg); /* IE 9 */\n  -webkit-transform: rotate(-8deg); /* Safari */\n  transform: rotate(-8deg);\n}\n}\n}\n\n</style>\n"]}]}